<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="au.edu.utas.username.kit305assig02">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewRaffle"></activity>
        <activity android:name=".RaffleDetails" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".NewTicket" android:parentActivityName=".RaffleDetails"></activity>
        <activity android:name=".RaffleUpdate" android:parentActivityName=".RaffleDetails"></activity>
        <activity android:name=".RaffleDraw" android:parentActivityName=".RaffleDetails"></activity>
        <activity android:name=".TicketList" android:parentActivityName=".RaffleDetails"></activity>
        <activity android:name=".ImageSelect" android:parentActivityName=".RaffleDetails"></activity>
        <activity android:name=".TicketUpdate" android:parentActivityName=".TicketList"></activity>
    </application>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
</manifest>